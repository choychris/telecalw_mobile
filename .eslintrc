{
    "env": {
        "node": true, 
        "browser": true, 
        "es6": true,
        "jest": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": ["react", "jsx-a11y", "import"],
    "globals": {
        "__DEV__": true
    },
    "rules": {
        "react/jsx-wrap-multilines": 0,
        "react/jsx-filename-extension": 0,
        "no-use-before-define": 0,
        "react/prop-types": 0
      },
    "settings": {
        "import/resolver": {
            "node": {
                "extensions": [".js", ".ios.js", ".android.js"]
            }
        }
    }
}